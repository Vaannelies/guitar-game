{
    "compilerOptions": {
        "strict": true,
        "target": "es6",
        "removeComments": true,
        "noUnusedLocals":true,
        "noUnusedParameters":true,
        "sourceMap": true,
        "outFile": "docs/js/main.js",
        "outDir": "docs",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true

    },
    "include": [
        "dev/**/*"
    ]
}